#include <stdio.h>

int main()
{
    float nota1, nota2, media;
    printf("Digite a nota1/n:");
    scanf("%f", & nota1);
    
    printf("Digite a nota2/n:");
    scanf("%f", & nota2);
    
    media=(nota1+nota2)/2;
    printf("A media Ã©: %1.f", media);
    
    if(media>5){
    printf("Aprovado");
    
    } else{
        printf("Reprovado");
    }
    return 0;
}
