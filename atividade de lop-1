1- escrever um programa para ler 2 valores 

#include <stdio.h>
#include <locale.h>

   int main(){
   int a,b;
   printf("digite um numero:");
   scanf("%d", &a);
   
   printf("digite outro numero:");
   scanf("%d", &b);
   
   printf("o maior numero e: ");
   
   if(a>b)
   printf("%d", a);
   
   else
   printf("%d", b);
   
    return 0;
}
